         type: animal \n        }) \n\n        setPets(animals || [])\n    }\n\n    useEffect(() => {\n        setBreeds([])\n        setBreed(\"\") \n\n        pet.breeds(animal).then(({ breeds: apiBreeds }) => {\n            const breedStrings = apiBreeds.map(({ name }) => name) \n            setBreeds(breedStrings)\n        }, console.error)\n    }, [animal, setBreed, setBreeds])\n\n\n    return (\n        <div className=\"search-params\">\n            <form onSubmit={(event) => {\n                event.preventDefault() \n                requestPets()\n            }}>\n                <label htmlFor=\"location\">\n                    Location\n                    <input id=\"location\" \n                    value={location} \n                    placeholder=\"Location\" \n                    onChange={event => setLocation(event.target.value)} />\n                </label> \n                    <AnimalDropdown /> \n                    <BreedDropdown />\n                <button>Submit</button>\n            </form> \n            <Results pets={pets} /> \n        </div>\n    )\n}\nexport default SearchParams; "},"lineCount":null}},"error":null,"hash":"52163ef0d0d22f290ece87316eff6440","cacheData":{"env":{}}}