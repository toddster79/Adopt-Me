dal} = this.state \n\n        return ( \n            <div className=\"details\"> \n                <Carousel media={media} />\n                <div>\n                    <h1>{name}</h1> \n                    <h2>{`${animal} - ${breed} - ${location}`}</h2>\n                    <button onClick={this.toggleModal}>Adopt {name}</button> \n                    <p>{description}</p> \n                    {\n                        showModal ? (\n                            <Modal>\n                                <div>\n                                    <h1>Would you like to adopt {name}?</h1> \n                                    <div className=\"buttons\">\n                                        <button onClick={this.adopt}>Yes</button>\n                                        <button onClick={this.toggleModal}>No, I'm not ready</button>\n                                    </div>\n                                </div>\n                            </Modal>\n                        ) : null\n                    }\n                </div> \n            </div>\n        )\n    }\n}\n\nexport default Details "},"lineCount":null}},"error":null,"hash":"46253f56e3a7d7650648d151dfb3b9f0","cacheData":{"env":{}}}